@model IEnumerable<RentaCar.Models.Car>
@{
    ViewBag.Title = "Cars";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Cars</h2>

<table class="table table-bordered table-hover">

    <thead>

        <tr>

            <th>Car</th>
            <th>Type</th>

        </tr>

    </thead>

    <tbody>

        @foreach (var car in Model)

        {

            <tr>

                <td>@Html.ActionLink(car.Name, )</td>
                <td>@car.TypeOfCar.Type</td>

            </tr>

        }

    </tbody>

</table>

